{
    "name": "tutus/adminkit",
    "description": "Minimal Laravel authentication scaffolding with Blade and Tailwind or API only.",
    "keywords": ["laravel", "admin kit", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Akshay Shah",
            "email": "shahakshay938@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.35",
        "illuminate/filesystem": "^10.35",
        "illuminate/support": "^10.35",
        "illuminate/validation": "^10.35",
        "laravel/prompts": "^0.1.13"
    },
    "autoload": {
        "psr-4": {
            "Tutus\\Adminkit\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Tutus\\Adminkit\\AdminKitServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
